<div class="col-md-12">
  <table class="table table-hover">
    <thead>
      <tr>
        <td>姓名</td>
        <td>性别</td>
        <td>年龄</td>
        <td>毕业学校</td>
        <td>主修专业</td>
        <td>联系电话</td>
        <td>简历</td>
      </tr>
    </thead>
    <tbody>
        <tr>

          <td><%= @resume.name %></td>
          <td><%= @resume.gender %></td>
          <td><%= @resume.age %></td>
          <td><%= @resume.school %></td>
          <td><%= @resume.major %></td>
          <td><%= @resume.phonenumber %></td>
          <td>
            <% if @resume.attachment.present? %>
              已提交简历
            <% else %>
              没有提交简历
            <% end %>
          </td>
        </tr>
    </tbody>
  </table>
</div>
